 109:Core/u8g2/u8x8_8x8.c **** /*
 110:Core/u8g2/u8x8_8x8.c ****   Source: http://graphics.stanford.edu/~seander/bithacks.html
 111:Core/u8g2/u8x8_8x8.c **** 	Section: Interleave bits by Binary Magic Numbers 
 112:Core/u8g2/u8x8_8x8.c ****    Original codes is here:
 113:Core/u8g2/u8x8_8x8.c **** 		static const unsigned int B[] = {0x55555555, 0x33333333, 0x0F0F0F0F, 0x00FF00FF};
 114:Core/u8g2/u8x8_8x8.c **** 		static const unsigned int S[] = {1, 2, 4, 8};
 115:Core/u8g2/u8x8_8x8.c **** 
 116:Core/u8g2/u8x8_8x8.c **** 		unsigned int x; // Interleave lower 16 bits of x and y, so the bits of x
 117:Core/u8g2/u8x8_8x8.c **** 		unsigned int y; // are in the even positions and bits from y in the odd;
 118:Core/u8g2/u8x8_8x8.c **** 		unsigned int z; // z gets the resulting 32-bit Morton Number.  
 119:Core/u8g2/u8x8_8x8.c **** 				// x and y must initially be less than 65536.
 120:Core/u8g2/u8x8_8x8.c **** 
 121:Core/u8g2/u8x8_8x8.c **** 		x = (x | (x << S[3])) & B[3];
 122:Core/u8g2/u8x8_8x8.c **** 		x = (x | (x << S[2])) & B[2];
 123:Core/u8g2/u8x8_8x8.c **** 		x = (x | (x << S[1])) & B[1];
 124:Core/u8g2/u8x8_8x8.c **** 		x = (x | (x << S[0])) & B[0];
 125:Core/u8g2/u8x8_8x8.c **** 
 126:Core/u8g2/u8x8_8x8.c **** 		y = (y | (y << S[3])) & B[3];
 127:Core/u8g2/u8x8_8x8.c **** 		y = (y | (y << S[2])) & B[2];
