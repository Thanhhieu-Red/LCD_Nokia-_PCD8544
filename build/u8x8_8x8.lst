 130:Core/u8g2/u8x8_8x8.c **** 
 131:Core/u8g2/u8x8_8x8.c **** 		z = x | (y << 1);
 132:Core/u8g2/u8x8_8x8.c **** */
 133:Core/u8g2/u8x8_8x8.c **** uint16_t u8x8_upscale_byte(uint8_t x) 
 134:Core/u8g2/u8x8_8x8.c **** {
 135:Core/u8g2/u8x8_8x8.c **** 	uint16_t y = x;
 136:Core/u8g2/u8x8_8x8.c **** 	y |= (y << 4);		// x = (x | (x << S[2])) & B[2];
 137:Core/u8g2/u8x8_8x8.c **** 	y &= 0x0f0f;
 138:Core/u8g2/u8x8_8x8.c **** 	y |= (y << 2);		// x = (x | (x << S[1])) & B[1];
 139:Core/u8g2/u8x8_8x8.c **** 	y &= 0x3333;
