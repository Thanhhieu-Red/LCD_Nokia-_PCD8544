 141:Core/u8g2/u8x8_8x8.c **** 	y &= 0x5555;
 142:Core/u8g2/u8x8_8x8.c ****   
 143:Core/u8g2/u8x8_8x8.c **** 	y |= (y << 1);		// z = x | (y << 1);
 144:Core/u8g2/u8x8_8x8.c **** 	return y;
 145:Core/u8g2/u8x8_8x8.c **** }
